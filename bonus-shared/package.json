{
  "name": "bonus-shared",
  "version": "1.0.0",
  "description": "Shared bonus types and client-safe eligibility validators",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./validators": {
      "types": "./dist/validators/index.d.ts",
      "import": "./dist/validators/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "peerDependencies": {},
  "keywords": [
    "bonus",
    "eligibility",
    "validation",
    "shared",
    "client",
    "server"
  ],
  "license": "MIT"
}

